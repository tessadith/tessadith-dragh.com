{
  "name": "tessadith-dragh.com",
  "description": "Source code of tessadith-dragh.com.me",
  "repository": {
    "type": "git",
    "url": "git://github.com/tessadith/tessadith-dragh.com.git"
  },
  "browser": {
    "fss": "./src/javascript/lib/fss.min.js",
    "jfp": "./src/javascript/lib/jquery-floating-placeholder.min.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "uglifyify"
    ]
